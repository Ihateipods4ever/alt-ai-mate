{
  "name": "alt-ai-mate-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "install": "npm install --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}